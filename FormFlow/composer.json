{
	"name": "craue/formflow",
	"type": "library",
	"description": "Core library for CraueFormFlowBundle.",
	"keywords": ["form"],
	"homepage": "https://github.com/craue/CraueFormFlow",
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Raue",
			"email": "christian.raue@gmail.com"
		},
		{
			"name": "Symfony Community",
			"homepage": "https://github.com/craue/CraueFormFlow/contributors"
		}
	],
	"require": {
		"php": ">=5.3.2",
		"symfony/event-dispatcher": "~2.1",
		"symfony/form": "~2.1",
		"symfony/http-foundation": "~2.1",
		"symfony/http-kernel": "~2.1",
		"symfony/options-resolver": "~2.1",
		"symfony/translation": "~2.1",
		"symfony/yaml": "~2.1"
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"autoload": {
		"psr-0": {
			"Craue\\FormFlow": ""
		}
	},
	"target-dir": "Craue/FormFlow",
	"extra": {
		"branch-alias": {
			"dev-master": "3.0.x-dev"
		}
	}
